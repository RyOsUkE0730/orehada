<div class="item tweet-box-wrapper">
  <div class="img-preview">
    <%= image_tag nikibi.image %>
  </div>
  <div class="tweet-description-wrapper">
    <%= link_to "もっと見る", item_path(nikibi.id), class:"tweet-description-subheader" %>
    <p class="tweet-description-header"><%= nikibi.title %></p>
    <p class="tweet-description-info"><%= nikibi.explanation %></p>
  </div>
</div>